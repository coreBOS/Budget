<tables>
<table>
<name>vtiger_cbbudget</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbbudget` (
  `cbbudgetid` int(11) NOT NULL,
  `name` varchar(150) default NULL,
  `businessunit` int(11) default NULL,
  `fromdate` DATE default NULL,
  `budgettype` varchar(50) default NULL,
  `todate` DATE default NULL,
  `budgetstatus` varchar(56) default NULL,
  `cbbudgetno` varchar(56) default NULL,
  `eincome` decimal(25,6) default NULL,
  `rincome` decimal(25,6) default NULL,
  `eexpense` decimal(25,6) default NULL,
  `rexpense` decimal(25,6) default NULL,
  `etotal` decimal(25,6) default NULL,
  `rtotal` decimal(25,6) default NULL,
  `deviationamount` decimal(25,6) default NULL,
  `deviationpercentage` decimal(6,2) default NULL,
  PRIMARY KEY (`cbbudgetid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbbudgetcf</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbbudgetcf` (
  `cbbudgetid` int(11) NOT NULL,
  PRIMARY KEY (`cbbudgetid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
